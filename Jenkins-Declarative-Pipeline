pipeline{
    agent any
    
    environment{
        PATH = "/usr/share/maven/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/Arvind3534/myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
            sh "mvn clean package"
            }
        }
    }
}
