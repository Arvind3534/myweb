pipeline{
    agent any
    
    tools {
        maven 'M2_HOME'
    }
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/Arvind3534/myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
            sh "mvn clean package"
            }
        }
    }
}
